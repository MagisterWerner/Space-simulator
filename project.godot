config_version=5

[application]

config/name="Intergalactic Space Haulers"
config/version="0.0.2"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.4")
boot_splash/bg_color=Color(0.0862745, 0.0862745, 0.0862745, 1)
config/icon="res://assets/sprites/player_ship.png"

[audio]

buses/default_bus_layout=""

[autoload]

GameManager="*res://autoload/game_manager.gd"
Logger="*res://scripts/utils/debug_logger.gd"
SeedManager="*res://autoload/seed_manager.gd"
GridManager="*res://autoload/grid_manager.gd"
EntityManager="*res://autoload/entity_manager.gd"
EventManager="*res://autoload/event_manager.gd"
ResourceManager="*res://autoload/resource_manager.gd"
AudioManager="*res://autoload/audio_manager.gd"
LabelManager="*res://autoload/label_manager.gd"

[category]

property_name=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/always_on_top=true
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray("res://addons/gdfxr/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "purple",
"res://autoload/": "green",
"res://resources/": "blue",
"res://scenes/": "red",
"res://scripts/": "green",
"res://scripts/data/": "red",
"res://scripts/generators/": "red"
}

